{
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:vue/vue3-strongly-recommended",
      "@vue/typescript/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 12,
      "parser": "@typescript-eslint/parser"
    },
    "plugins": [
      "vue",
      "@typescript-eslint"
    ]
}
  